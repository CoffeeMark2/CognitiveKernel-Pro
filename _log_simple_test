[1;37;44mprint[0m[1;37;44m([0m[1;37;44mclick[0m[1;37;44m([0m[37;44mid[0m[37;44m=[0m[1;37;44m27[0m[37;44m, [0m[37;44mlink_name[0m[37;44m=[0m[37;44m"SERP[0m[37;44m,5573"[0m[1;37;44m)[0m[1;37;44m)[0m[37;44m```[0m
